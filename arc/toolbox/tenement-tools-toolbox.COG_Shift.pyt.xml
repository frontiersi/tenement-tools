<metadata xml:lang="en"><Esri><CreaDate>20220511</CreaDate><CreaTime>11532300</CreaTime><ArcGISFormat>1.0</ArcGISFormat><SyncOnce>TRUE</SyncOnce><ModDate>20220511</ModDate><ModTime>12265900</ModTime><scaleRange><minScale>150000000</minScale><maxScale>5000</maxScale></scaleRange><ArcGISProfile>ItemDescription</ArcGISProfile></Esri><tool name="COG_Shift" displayname="COG Shift" toolboxalias="toolbox" xmlns=""><arcToolboxHelpPath>c:\program files\arcgis\pro\Resources\Help\gp</arcToolboxHelpPath><parameters><param name="in_nc_file" displayname="Input satellite NetCDF file" type="Required" direction="Input" datatype="File" expression="in_nc_file"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;A raw satellite data NetCDF generated by the COG Fetch module. Input NetCDF must have coordinates for x, y. &lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="out_nc_file" displayname="Output shifted NetCDF file" type="Required" direction="Output" datatype="File" expression="out_nc_file"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The NetCDF file containing the shifted satellite data outputs.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="in_shift_x" displayname="Shift X (metres)" type="Required" direction="Input" datatype="Double" expression="in_shift_x"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The value used to shift the x-coordinates. Use a negative value to shift west and a positive value to shift east.&lt;/SPAN&gt;&lt;/P&gt;&lt;P&gt;&lt;SPAN&gt;This value is in metres.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="in_shift_y" displayname="Shift Y (metres)" type="Required" direction="Input" datatype="Double" expression="in_shift_y"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt